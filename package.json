{
  "name": "desafio-genjutsu",
  "version": "1.0.0",
  "description": "Desafio do Genjutsu de Itachi - Encontre o Pangrama!",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "sass": "sass styles.scss styles.css",
    "sass:watch": "sass --watch styles.scss:styles.css",
    "build:css": "sass styles.scss styles.css --style compressed",
    "dev:all": "concurrently \"npm run sass:watch\" \"npm run dev\"",
    "test": "node test-pangramas.js"
  },
  "keywords": [
    "pangrama",
    "desafio",
    "itachi",
    "genjutsu",
    "tsukuyomi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sass": "^1.69.5",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
